      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="CONSTANT_C1">
    <cd:Integer>
      <cd:Description>Constant C1 for radiance to brightness temperature conversion (C1=2hc^2, h: Plank's constant, c: speed of light)</cd:Description>
      <cd:Unit>1e-11 mW/(m2 sr cm-4)</cd:Unit>
      <cd:Conversion numerator="1" denominator="1e11">
        <cd:Unit>mW/(m2 sr cm-4)</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
