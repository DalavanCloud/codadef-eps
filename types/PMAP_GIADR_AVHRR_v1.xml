  <cd:Field name="CONSTANT_C2">
    <cd:Integer>
      <cd:Description>Constant C2 for radiance to brightness temperature conversion (C2=hc/k, h: Plank's constant, k: Boltzmann's constant)</cd:Description>
      <cd:Unit>1e-6 K/cm-1</cd:Unit>
      <cd:Conversion numerator="1" denominator="1e6">
        <cd:Unit>K/cm-1</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
